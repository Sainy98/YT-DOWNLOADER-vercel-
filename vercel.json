{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/css/",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" },
      "dest": "/static/css/$1"
    },
    {
      "src": "/",
      "dest": "/app.py"
    }
  ]
}
